{
	"env": {
		"es2021": true
	},
	"extends": ["standard-with-typescript", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"@typescript-eslint/consistent-type-definitions": ["error", "type"]
	},
	"ignorePatterns": ["testing-library/src/testing/*.ts", "node_modules"]
}
