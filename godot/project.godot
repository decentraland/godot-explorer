; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Decentraland"
config/description="Decentraland Explorer powered by Decentraland DAO"
run/main_scene="res://src/main.tscn"
config/features=PackedStringArray("4.4")
boot_splash/fullsize=false
boot_splash/image="res://assets/ui/isologotipo.png"
config/icon="res://decentraland_logo_squared.png"

[audio]

driver/enable_input=true
driver/mix_rate=48000
driver/enable_input.ios=false

[autoload]

UiSounds="*res://src/helpers_components/ui_sounds.gd"
Global="*res://src/global.gd"
XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
XRToolsRumbleManager="*res://addons/godot-xr-tools/rumble/rumble_manager.gd"

[debug]

gdscript/warnings/unused_variable=2
gdscript/warnings/unused_signal=0
gdscript/warnings/shadowed_variable=2
gdscript/warnings/redundant_await=false

[display]

window/size/viewport_width=720
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720
window/handheld/orientation=6

[editor]

run/main_run_args="--realm https://sdk-team-cdn.decentraland.org/ipfs/goerli-plaza-main-latest --position 56,-54 --skip-lobby"

[editor_plugins]

enabled=PackedStringArray("res://addons/dcl_build_plugin/plugin.cfg", "res://addons/resource_tracker/plugin.cfg", "res://addons/webview-godot-android/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://assets/themes/theme.tres"
theme/custom_font="res://assets/themes/fonts/inter/Inter-Regular.ttf"

